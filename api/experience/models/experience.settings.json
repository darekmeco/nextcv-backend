{
  "connection": "default",
  "collectionName": "experiences",
  "info": {
    "name": "experience"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "position": {
      "type": "string"
    },
    "resume_id": {
      "via": "experiences",
      "model": "resume"
    },
    "company_name": {
      "type": "string"
    },
    "place": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "start": {
      "type": "string"
    },
    "end": {
      "type": "string"
    }
  }
}
